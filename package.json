{
  "name": "codespartans-organization-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "dev:watch": "bun run get-schema && bunx concurrently \"bun run codegen:watch\" \"bun run dev\"",
    "get-schema": "bun scripts/get-schema.ts",
    "codegen": "bunx graphql-codegen --config codegen.ts",
    "codegen:get-schema": "bun run get-schema && bun run codegen",
    "codegen:watch": "bunx graphql-codegen --config codegen.ts --watch",
    "codegen:get-schema:watch": "bun run get-schema && bun run codegen:watch"
  },
  "dependencies": {
    "@apollo/client": "3.10.8",
    "@graphql-codegen/cli": "^6.0.0",
    "@graphql-codegen/typescript-graphql-files-modules": "^3.0.1",
    "@hookform/resolvers": "^5.2.2",
    "@parcel/watcher": "^2.5.1",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.3",
    "graphql-ws": "5.16.0",
    "lucide-react": "^0.546.0",
    "next": "15.5.6",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.65.0",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "graphql-tag": "^2.12.6",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5"
  }
}
